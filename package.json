{
  "name": "ts-p5js",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint src/*.ts",
    "format": "prettier src/*.ts",
    "build": "webpack",
    "watch": "concurrently \"browser-sync start --server -w\" \" webpack --watch\""
  },
  "license": "MIT",
  "devDependencies": {
    "@types/p5": "^0.7.2",
    "@typescript-eslint/eslint-plugin": "^1.8.0",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "p5": "^0.8.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
